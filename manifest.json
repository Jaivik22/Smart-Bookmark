{
    "manifest_version": 3,
    "name": "Smart Bookmark Saver",
    "version": "1.0",
    "description": "Save bookmarks with custom sections",
    "permissions": ["storage", "bookmarks", "tabs"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "assets/bookmark.png"
    },
    "icons": {
      "128": "assets/bookmark.png"
    },
    "browser_specific_settings": {
  "gecko": {
    "id": "bookmark-saver@jaivik",
    "strict_min_version": "57.0"
  }
}

  }